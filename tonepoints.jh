entity User {
name String,
email String
}

entity Settings{
weekly_goal Integer required,
weight_units Units required
}

entity Points{
date LocalDate required,
exercise Integer,
meals Integer,
alcohol Integer,
notes String
}
entity Weight {
weekly_goal Integer,
weight_units Units
}
entity Blood_Pressure {
date LocalDate required,
systolic Integer,
diastolic Integer
}
relationship ManyToOne{
Points to User
Weight to User
Blood_Pressure to User 
}
relationship OneToOne {
Settings to User
}